{
  "name": "nestjs_irsx",
  "version": "2.3.0",
  "description": "Nestjs Boilerplate",
  "author": "Galang Ardian",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && nest build",
    "dev": "pnpm start:dev",
    "lint": "eslint . --ext js --ext ts --fix && prettier --config ./.prettierrc.js --write **/*.[jt]s --log-level silent",
    "postinstall": "husky install",
    "prebuild": "rimraf dist",
    "preinstall": "npx only-allow pnpm",
    "prod": "pnpm start:prod",
    "repl": "pnpm start:repl",
    "commit": "cz --no-verify",
    "start": "NODE_ENV=production nest start",
    "start:dev": "nest start --debug --watch",
    "start:prod": "pnpm build && pnpm start",
    "start:repl": "nest start --watch --entryFile ./src/utils/repl",
    "swagger": "node -r @swc-node/register ./src/utils/swagger",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:e2e": "jest --config ./test/jest.e2e.config.ts",
    "test:unit": "jest",
    "typeorm": "ts-node --transpile-only -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/db/database_config.ts",
    "schema:sync": "npm run typeorm schema:sync",
    "add:migration": "npm run build && npm run typeorm migration:generate -- -n",
    "apply:migration": "npm run build && npm run typeorm migration:run",
    "revert:migration": "npm run build && npm run typeorm migration:revert",
    "drop:database": "npm run typeorm schema:drop"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.1",
    "@nestjs-plus/rabbitmq": "^1.4.4",
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/microservices": "^10.1.3",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/typeorm": "^8.0.2",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "axios": "^1.4.0",
    "big.js": "^6.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.9",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "mathjs": "^11.9.1",
    "nestjs-joi": "^1.9.0",
    "nestjs-pino": "^3.3.0",
    "openid-client": "^5.4.3",
    "pg": "^8.11.2",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typeorm": "^0.2.37",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@commitlint/cz-commitlint": "^17.6.7",
    "@commitlint/types": "^17.4.4",
    "@nestjs/cli": "^10.1.8",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.5",
    "@swc-node/register": "^1.6.6",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.69",
    "@types/big.js": "^6.2.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "pino-pretty": "^10.0.1",
    "pino-webpack-plugin": "^2.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "swc-loader": "^0.2.3",
    "ts-jest": "^29.1.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.1.6",
    "webpack": "5.88.1"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
